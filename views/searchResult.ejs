		<td class=<%=stripe%>><%= customer.adress %></td>
		<td class=<%=stripe%>><%= customer.postnummer %></td>
		<td class=<%=stripe%>><%= customer.telefonnummer %></td>
		<%if (stripe == "tg-b7b8"){ %>
		<% stripe = "tg-yzt1" %>
		<% }else { %>
		<% stripe = "tg-b7b8" %>
		<%}%>
	</tr>
	<%}%>
	<% }) %>
